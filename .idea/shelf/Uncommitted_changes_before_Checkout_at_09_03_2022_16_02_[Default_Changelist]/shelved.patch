<+>{% extends 'base.html.twig' %}\r\n\r\n{% block title %}article index{% endblock %}\r\n\r\n{% block body %}\r\n    <h2>\r\n        article\r\n    </h2>\r\n\r\n\r\n    <table class=\"table table-bordered\">\r\n        <thead>\r\n        <tr>\r\n            <th>Id</th>\r\n            <th>Nom</th>\r\n            <th>Image</th>\r\n            <th>Prix d'Achat</th>\r\n            <th>Prix de Vente</th>\r\n            <th>Quantite Initial</th>\r\n            <th>Quantite Restant</th>\r\n            <th></th>\r\n        </tr>\r\n        </thead>\r\n        <tbody>\r\n        {% for article in articles %}\r\n            <tr>\r\n                <td>{{ article.id }}</td>\r\n                <td>{{ article.nom }}</td>\r\n                <td class=\"w-5\"><img style=\"width: 70px;\" class=\"img-fluid img-thumbnail\" src=\"{{ vich_uploader_asset(article, 'imageFile') }}\" alt=\"{{ article.nom }}\"></td>\r\n                <td>{{ article.prisAchat }}</td>\r\n                <td>{{ article.prisVente }}</td>\r\n                <td>{{ article.quantiteInitial }}</td>\r\n                <td>{{ article.quantiteRestant }}</td>\r\n                <td>\r\n                    <a href=\"{{ path('admin_article_edit', {'id': article.id}) }}\">\r\n                        <button type=\"submit\" class=\"btn btn-primary me-2\">Moifier</button>\r\n                    </a>\r\n                </td>\r\n                <td>\r\n                    <form method=\"post\" action=\"{{ path('admin_article_delete', {'id': article.id}) }}\"\r\n                          onsubmit=\"return confirm('Voulez vous vaiment suprimer?');\">\r\n                        <input type=\"hidden\" name=\"_token\" value=\"{{ csrf_token('delete' ~ article.id) }}\">\r\n                        <button type=\"submit\" class=\"btn btn-danger me-2\">Suprimer</button>\r\n                    </form>\r\n                </td>\r\n            </tr>\r\n        {% else %}\r\n            <tr>\r\n                <td colspan=\"7\">no records found</td>\r\n            </tr>\r\n        {% endfor %}\r\n        </tbody>\r\n    </table>\r\n\r\n    <a href=\"{{ path('admin_article_new') }}\">\r\n        <button type=\"submit\" class=\"btn btn-primary me-2\">Ajouter</button>\r\n    </a>\r\n{% endblock %}\r\n
